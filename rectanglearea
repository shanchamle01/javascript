        const length = 10; 
        const width = 5;   
        const rectangleArea = length * width;

        document.write("Length: " + length + "<br>");
        document.write("Width: " + width + "<br>");
        document.write("Area of Rectangle: " + rectangleArea + "<br>");
    </script>
